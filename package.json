{
  "name": "ssnit_calculator",
  "version": "1.0.0",
  "description": "Npm package to calculate ssnit contribution in ghana",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node ./index.js",
    "build": "babel src --out-dir ./dist --source-maps",
    "serve": "node ./dist/index.js",
    "flow": "flow",
    "test": "nodemon ./node_modules/.bin/mocha --require @babel/register --recursive --growl --reporter spec",
    "debug": "node --inspect-brk ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Philip-Nunoo/ssnit_calculator.git"
  },
  "keywords": [
    "ssnit",
    "ghana"
  ],
  "author": "Philip <ketay99@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Philip-Nunoo/ssnit_calculator/issues"
  },
  "homepage": "https://github.com/Philip-Nunoo/ssnit_calculator#readme",
  "dependencies": {
    "decimal.js": "^10.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "chai": "^4.2.0",
    "flow-bin": "^0.102.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1"
  }
}
